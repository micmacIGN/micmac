<ParamMICMAC>

<DicoLoc>
<!-- Entrees minimales -->
<!--
   <Symb> Dir=""     </Symb>
   <Symb> ImPat="IMG_[0-9]{4}.CR2"     </Symb>
   <Symb> Ori=All     </Symb>

