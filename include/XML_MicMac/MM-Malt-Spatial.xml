                 <ModeInterpolation Portee="Globale"> eInterpolSinCard </ModeInterpolation>
                 <#WHEN VTEST=${ForDeform}>
                       <SzSinCard Portee="Globale">  5.0 </SzSinCard>
                       <SzAppodSinCard Portee="Globale">  5.0 </SzAppodSinCard>
