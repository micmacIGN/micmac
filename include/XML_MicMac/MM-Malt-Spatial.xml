                   <SzBlocAH > 128 </SzBlocAH>
                   <TypeCAH>
                           <GPU_CorrelBasik >  true   </GPU_CorrelBasik>
                   </TypeCAH>
               </CorrelAdHoc>
            </#WHEN>
            
        </EtapeMEC>
	     
<!--
        <EtapeMEC> 
                <DeZoom >        ${ZoomInit}        </DeZoom> 
                <ZPas>           ${DbleZPas}       </ZPas>
        </EtapeMEC>
-->
        <EtapeMEC> 
                <DeZoom >        ${ZoomInit}        </DeZoom> 
        </EtapeMEC>

        <#WHEN VTEST=${AddZoom64}>
              <EtapeMEC>
                  <DeZoom >        64        </DeZoom>
                  <ModeInterpolation> eInterpolBiLin </ModeInterpolation>
                  <ZPas>        ${DbleZPas} </ZPas>

              </EtapeMEC>
        </#WHEN>

        <#WHEN VTEST=${AddZoom32}>
              <EtapeMEC>
                  <DeZoom >        32        </DeZoom>
                  <ZPas>  ${DbleZPas} </ZPas>
