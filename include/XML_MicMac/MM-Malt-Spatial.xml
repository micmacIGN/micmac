   <Symb>   CostTrans=4.0      </Symb>
   <Symb>   NbMinIV=2       </Symb>
   <Symb>   Repere="NO-REPERE"  </Symb>
   <Symb>   FileAnam=EmptyXML.xml  </Symb>
   <Symb>   FileZMoy=EmptyXML.xml  </Symb>
   <Symb>   UseGpu=false </Symb>
